    for fich in `ls . `
    do
            if [ -f $fich ]
            then
                    echo ./w1w2 data_clean/random/$fich 1
            fi
    done

    for fich in `ls . `
    do
            if [ -f $fich ]
            then
                    echo ./w1w2 data_clean/random/$fich 2
            fi
    done

    for fich in `ls . `
    do
            if [ -f $fich ]
            then
                    echo ./hojny_flot data_clean/random/$fich 
            fi
    done

